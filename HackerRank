1. Plus Minus
#include <stdio.h>

void plusMinus(int arr[], int n) {
    int pos = 0, neg = 0, zero = 0;
    
    for (int i = 0; i < n; i++) {
        if (arr[i] > 0)
            pos++;
        else if (arr[i] < 0)
            neg++;
        else
            zero++;
    }
    
    printf("%.6f\n", (float)pos / n);
    printf("%.6f\n", (float)neg / n);
    printf("%.6f\n", (float)zero / n);
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];  // Variable Length Array (C99 or later)
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    plusMinus(arr, n);

    return 0;
}


2. Mini-Max Sum
#include <stdio.h>
#include <limits.h>  // For INT_MAX and INT_MIN

void miniMaxSum(int arr[], int n) {
    long long totalSum = 0;
    int min = INT_MAX;
    int max = INT_MIN;

    for (int i = 0; i < n; i++) {
        totalSum += arr[i];
        if (arr[i] < min)
            min = arr[i];
        if (arr[i] > max)
            max = arr[i];
    }

    long long minSum = totalSum - max;
    long long maxSum = totalSum - min;

    printf("%lld %lld\n", minSum, maxSum);
}

int main() {
    int arr[5];

    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    miniMaxSum(arr, 5);

    return 0;
}
