Mini-Max Sum
#include <stdio.h>
#include <limits.h>  // For INT_MAX and INT_MIN

void miniMaxSum(int arr[], int n) {
    long long totalSum = 0;
    int min = INT_MAX;
    int max = INT_MIN;

    for (int i = 0; i < n; i++) {
        totalSum += arr[i];
        if (arr[i] < min)
            min = arr[i];
        if (arr[i] > max)
            max = arr[i];
    }

    long long minSum = totalSum - max;
    long long maxSum = totalSum - min;

    printf("%lld %lld\n", minSum, maxSum);
}

int main() {
    int arr[5];

    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }

    miniMaxSum(arr, 5);

    return 0;
}
